{
  "name": "@n3bula/echo",
  "version": "0.1.0-alpha.4",
  "description": "Colorized log",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.js",
      "default": "./dist/node.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "log",
    "colorized",
    "echo",
    "n3bula"
  ],
  "scripts": {
    "dev:browser": "vite",
    "dev:node": "cross-env DEV=true nova",
    "build:browser": "tsc && vite build",
    "build:node": "cross-env DEV=false nova build",
    "build": "pnpm run build:browser && pnpm run build:node"
  },
  "devDependencies": {
    "@n3bula/nova": "workspace:*",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "contributors": [
    {
      "name": "Moriarty47",
      "email": "wang_jn_xian@163.com"
    }
  ],
  "homepage": "https://github.com/Moriarty47/n3bula/tree/main/packages/echo#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moriarty47/n3bula.git",
    "directory": "packages/echo"
  },
  "author": {
    "name": "Moriarty47",
    "email": "wang_jn_xian@163.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
