{
  "name": "@n3bula/express",
  "version": "0.0.2-alpha.1",
  "description": "A Node.js server based on Express.js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "default": "./loader/index.js"
    },
    "./global": {
      "types": "./global.d.ts"
    }
  },
  "files": [
    "dist",
    "loader",
    "global.d.ts"
  ],
  "scripts": {
    "dev": "nova",
    "build": "nova build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "Express.js",
    "n3bula",
    "server"
  ],
  "contributors": [
    {
      "name": "Moriarty47",
      "email": "wang_jn_xian@163.com"
    }
  ],
  "homepage": "https://github.com/Moriarty47/n3bula/tree/main/packages/express#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moriarty47/n3bula.git",
    "directory": "packages/express"
  },
  "author": {
    "name": "Moriarty47",
    "email": "wang_jn_xian@163.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@n3bula/echo": "workspace:*",
    "@n3bula/nova": "workspace:*",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "express": "^4.19.2",
    "globby": "^16.0.0",
    "nanoid": "^5.1.6",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0"
  }
