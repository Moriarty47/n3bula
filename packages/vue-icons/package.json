    "./index.css": "./dist/index.css",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "ts-node src/build.ts",
    "update-icons": "ts-node src/update.ts",
    "preview": "cd example && pnpm start"
  },
