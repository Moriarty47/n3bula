{
  "name": "@n3bula/vue-icons",
  "version": "0.0.1-alpha.2",
  "description": "Vue icon components based on Geist Design System Icons.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
