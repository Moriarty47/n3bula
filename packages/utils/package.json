{
  "name": "@n3bula/utils",
  "version": "0.0.7-alpha.2",
  "description": "A utils library for n3bula packages.",
  "main": "./lib/umd/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/umd/index.js",
      "default": "./lib/esm/index.js"
    },
    "./*": {
      "types": "./lib/esm/*.d.ts",
      "import": "./lib/esm/*.js",
      "require": "./lib/umd/*.js",
      "default": "./lib/esm/*.js"
    }
  },
  "files": [
    "lib/*"
  ],
  "scripts": {
    "dev": "webpack  --define-process-env-node-env=development --watch",
    "start": "webpack  --define-process-env-node-env=production",
    "build": "shx rm -rf ./lib && pnpm run start && pnpm run build:esm",
    "build:esm": "pnpm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json",
    "test": "ava",
    "test:w": "cross-env NODE_OPTIONS='--import=tsx' ava --watch"
  },
  "keywords": [
    "utils",
    "n3bula"
  ],
  "contributors": [
    {
      "name": "Moriarty47",
      "email": "wang_jn_xian@163.com"
    }
  ],
  "homepage": "https://github.com/Moriarty47/n3bula/tree/main/packages/utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moriarty47/n3bula.git",
    "directory": "packages/utils"
  },
  "author": {
    "name": "Moriarty47",
    "email": "wang_jn_xian@163.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "ava": "^6.1.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "jsencrypt": "^3.3.2"
  }
}
